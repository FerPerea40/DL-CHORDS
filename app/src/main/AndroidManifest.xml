<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="DLChordsTT">
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
<uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

<application
    android:name="com.example.DLChordsTT.DLChordsApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.DLChordsTT1">
    <activity
        android:name="com.example.DLChordsTT.MainActivity"
        android:exported="true"
        android:label="@string/app_name"
        android:theme="@style/Theme.DLChordsTT1">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="com.example.DLChordsTT.features.music_player.ui.screens.PlayerMusicActivity"
        android:exported="false"
        android:label="@string/title_activity_player_music"
        android:theme="@style/Theme.DLChordsTT1" />
    <service
        android:name="com.example.DLChordsTT.features.music_player.ui.service.MusicPlayerService"
        android:exported="false">
        <intent-filter>
            <action android:name="android.media.browser.MediaBrowserService" />
        </intent-filter>

    </service>
</application>

</manifest>
